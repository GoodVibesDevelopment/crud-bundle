{% extends 'VardiusCrudBundle::layout.html.twig' %}

{% block content %}
    <div class="list-content col-xs-12 col-sm-6 col-md-12">
        <table class="table table-striped table-condensed">
            <tbody>
            {% for entity in results %}
                <tr class='list-view-item'>
                    <td>{{ entity.id }}</td>
                    <td><h3>override this layout to display content</h3></td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}
